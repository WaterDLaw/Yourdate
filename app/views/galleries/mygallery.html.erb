<div class="row">
  <div class="col s12">
    <div class="col s2"></div>
    <div class="col s8">
    <% @gallery.photographs.each do |photo| %>

      <div class="row">
        <div class="col s3">
          <div class="card">
            <div class="card-image">
              <div class="profile-image-wrapper">
                <%= image_tag photo.image_url.to_s , class:"fit-image" %>
              </div>
            </div>
          </div>
        </div>
      </div>
    <% end %>

    <p>add photographs</p>
    <%= render 'photographs/form', photograph: @photograph, gallery: @gallery %>
    </div>
    <div class="col s2"></div>
  </div>
</div>
